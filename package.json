{
  "name": "coreone-wiper-path",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && npm run build:client && npm run build:ssr && npm run prerender",
    "build:client": "vite build",
    "build:ssr": "vite build --ssr src/entry-server.tsx --outDir dist/server --ssrManifest",
    "prerender": "node ./prerender.mjs",
    "preview": "vite preview",
    "lint": "biome lint src",
    "format": "biome format --write src"
  },
  "dependencies": {
    "@fontsource-variable/material-symbols-rounded": "^5.2.30",
    "@tailwindcss/vite": "^4.1.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "solid-js": "^1.9.10",
    "solid-styled-components": "^0.28.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^24.10.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-solid": "^2.11.10",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
