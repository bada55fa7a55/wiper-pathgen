{
  "name": "coreone-wiper-path",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && npm run build:client && npm run build:ssr && npm run prerender",
    "build:client": "vite build",
    "build:ssr": "vite build --ssr src/entry-server.tsx --outDir dist/server --ssrManifest",
    "prerender": "node ./prerender.mjs",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src",
    "format": "biome check --write src",
    "test": "vitest",
    "test:run": "vitest run",
    "test:gcode:fixture": "tsx --tsconfig tsconfig.app.json scripts/generate-gcode-fixture.ts",
    "knip": "knip"
  },
  "dependencies": {
    "@fontsource-variable/material-symbols-rounded": "^5.2.30",
    "@tailwindcss/vite": "^4.1.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "solid-js": "^1.9.10",
    "solid-styled-components": "^0.28.5",
    "solid-toast": "^0.5.0",
    "solidjs-use": "^2.3.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^24.10.10",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@vitest/coverage-v8": "^4.0.18",
    "knip": "^5.83.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-solid": "^2.11.10",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.18"
  }
}
